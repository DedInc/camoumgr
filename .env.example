# Camoufox Manager Configuration

# Profile storage file
CAMOUMGR_PROFILES_FILE=profiles.json

# Browser data directory
CAMOUMGR_DATA_DIR=camoufox_data

# Log directory
CAMOUMGR_LOG_DIR=logs

# Log level (DEBUG, INFO, WARNING, ERROR)
CAMOUMGR_LOG_LEVEL=INFO

# Proxy check timeout in seconds
CAMOUMGR_PROXY_TIMEOUT=10
